{
  "extends": ["prettier", "plugin:vue/recommended"],
  "plugins": ["prettier"],
  "parser": "vue-eslint-parser",
  "parserOptions": {
      "parser": "typescript-eslint-parser"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "linebreak-style": 0,
    "prettier/prettier": ["error", { "singleQuote": true }]
  }
}
