{
  "name": "gib_mern",
  "version": "1.0.0",
  "description": "A test project for the warehouse I used to work.",
  "main": "index.js",
  "scripts": {
    "start": "node --require dotenv/config dist/server.js",
    "dboff": "killall mongod",
    "format": "prettier --write --single-quote --tab-width=2 \"{build-utils,server,client}/**/*.{js,vue}\"",
    "webpack": "webpack --require dotenv/config",
    "clean": "shx rm -rf ./dist",
    "prod": "npm run webpack -- --env.mode production",
    "prod:server": "npm run prod -- --env.side server",
    "dev": "npm run webpack -- --env.mode development",
    "dev:db": "mongod",
    "dev:client": "webpack-dev-server --open --hot --env.side client --env.mode development",
    "dev:server": "npm run dev -- --env.side server",
    "dev:all": "npm-run-all --parallel dev:db dev:server dev:client",
    "lint": "eslint -c \"./.eslintrc.json\" \"{build-utils,server,client}/**/*.{js,vue}\" --quiet",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tearingItUp786/gib_mern.git"
  },
  "author": "Taranveer Bains",
